{
  "name": "idealista",
  "version": "1.0.0",
  "description": "Servicio para gestión de calidad de los anuncios",
  "main": "index.js",
  "repository": "https://github.com/folmosr/idealista.git",
  "author": "Fabián Olmos",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "body-parse": "^0.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "http-status": "^1.5.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.10.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "source-map-loader": "^1.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "scripts": {
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node-transpile-only ./src/index.ts",
    "start:dev:full": "concurrently -k -s first \"yarn run test:watch\" \"yarn run start:dev\"",
    "start": "node -r esm ./dist/src/index.js",
    "lint": "./node_modules/.bin/eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "./node_modules/.bin/eslint \"src/**/*.{js,ts}\" --fix",
    "test": "NODE_ENV=test ./node_modules/.bin/jest --ci --color --detectOpenHandles --forceExit --verbose",
    "test:watch": "yarn run jest --watch",
    "test:integration": "NODE_ENV=test ./node_modules/.bin/jest --roots=./__tests__/integration",
    "test:unit-coverage": "yarn run jest --coverage --roots=./__tests__/unit",
    "prettier": "prettier --config .prettierrc \"src/**/*.ts\" --write"
  }
}
